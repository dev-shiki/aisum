{"ast":null,"code":"import api from \"../api/axios\";\nimport UploadBox from \"../components/UploadBox.vue\";\nimport SummaryDisplay from \"../components/SummaryDisplay.vue\";\nimport LoadingSpinner from \"../components/LoadingSpinner.vue\";\nexport default {\n  components: {\n    UploadBox,\n    SummaryDisplay,\n    LoadingSpinner\n  },\n  data() {\n    return {\n      isLoading: false,\n      transcription: \"\",\n      summary: \"\",\n      finalSummary: \"\"\n    };\n  },\n  methods: {\n    async handleFileUpload(file) {\n      this.isLoading = true;\n      const formData = new FormData();\n      formData.append(\"file\", file);\n      try {\n        const response = await api.post(\"/summarize/\", formData, {\n          headers: {\n            \"Content-Type\": \"multipart/form-data\"\n          }\n        });\n        if (response.data.error) {\n          alert(response.data.error);\n        } else {\n          this.transcription = response.data.transcription || \"Tidak ada transkripsi.\";\n          this.summary = response.data.summary || \"Tidak ada ringkasan.\";\n          this.finalSummary = response.data.final_summary || \"Tidak ada ringkasan akhir.\";\n        }\n      } catch (error) {\n        alert(\"Terjadi kesalahan saat memproses file.\");\n      } finally {\n        this.isLoading = false;\n      }\n    }\n  }\n};","map":{"version":3,"names":["api","UploadBox","SummaryDisplay","LoadingSpinner","components","data","isLoading","transcription","summary","finalSummary","methods","handleFileUpload","file","formData","FormData","append","response","post","headers","error","alert","final_summary"],"sources":["D:\\app\\meeting-summarizer\\src\\views\\HomeView.vue"],"sourcesContent":["<template>\r\n    <div class=\"container\">\r\n        \r\n      <!-- Upload File -->\r\n      <UploadBox @file-selected=\"handleFileUpload\" />\r\n  \r\n      <!-- Animasi Loading -->\r\n      <LoadingSpinner :isLoading=\"isLoading\" />\r\n  \r\n      <!-- Output Ringkasan -->\r\n      <SummaryDisplay v-if=\"summary\" :transcription=\"transcription\" :summary=\"summary\" :finalSummary=\"finalSummary\" />\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import api from \"../api/axios\";\r\n  import UploadBox from \"../components/UploadBox.vue\";\r\n  import SummaryDisplay from \"../components/SummaryDisplay.vue\";\r\n  import LoadingSpinner from \"../components/LoadingSpinner.vue\";\r\n  \r\n  export default {\r\n    components: {\r\n      UploadBox,\r\n      SummaryDisplay,\r\n      LoadingSpinner,\r\n    },\r\n    data() {\r\n      return {\r\n        isLoading: false,\r\n        transcription: \"\",\r\n        summary: \"\",\r\n        finalSummary: \"\",\r\n      };\r\n    },\r\n    methods: {\r\n      async handleFileUpload(file) {\r\n        this.isLoading = true;\r\n  \r\n        const formData = new FormData();\r\n        formData.append(\"file\", file);\r\n  \r\n        try {\r\n          const response = await api.post(\"/summarize/\", formData, {\r\n            headers: { \"Content-Type\": \"multipart/form-data\" },\r\n          });\r\n  \r\n          if (response.data.error) {\r\n            alert(response.data.error);\r\n          } else {\r\n            this.transcription = response.data.transcription || \"Tidak ada transkripsi.\";\r\n            this.summary = response.data.summary || \"Tidak ada ringkasan.\";\r\n            this.finalSummary = response.data.final_summary || \"Tidak ada ringkasan akhir.\";\r\n          }\r\n        } catch (error) {\r\n          alert(\"Terjadi kesalahan saat memproses file.\");\r\n        } finally {\r\n          this.isLoading = false;\r\n        }\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n  "],"mappings":"AAeE,OAAOA,GAAE,MAAO,cAAc;AAC9B,OAAOC,SAAQ,MAAO,6BAA6B;AACnD,OAAOC,cAAa,MAAO,kCAAkC;AAC7D,OAAOC,cAAa,MAAO,kCAAkC;AAE7D,eAAe;EACbC,UAAU,EAAE;IACVH,SAAS;IACTC,cAAc;IACdC;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,KAAK;MAChBC,aAAa,EAAE,EAAE;MACjBC,OAAO,EAAE,EA